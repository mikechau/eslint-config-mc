{
  "name": "eslint-config-mc",
  "version": "0.0.1",
  "description": "eslint config based on Airbnb's style, with support for an all warning config",
  "main": "index.js",
  "files": [
    "dist",
    "src",
    "index.js",
    "warning.js"
  ],
  "scripts": {
    "test": "ava --verbose",
    "lint": "eslint ./src ./util ./test ./dist index.js",
    "build": "./scripts/build.sh",
    "check": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechau/eslint-config-mc.git"
  },
  "keywords": [
    "eslint",
    "airbnb",
    "config",
    "lint",
    "react",
    "es6"
  ],
  "author": "Mike Chau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikechau/eslint-config-mc/issues"
  },
  "homepage": "https://github.com/mikechau/eslint-config-mc#readme",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.14.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "lodash": "^4.12.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base/legacy",
    "env": {
      "node": true
    },
    "rules": {
      "space-before-function-paren": 0
    }
  }
}
