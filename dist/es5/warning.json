{
  "env": {
    "node": true,
    "browser": true,
    "amd": false,
    "mocha": false,
    "jasmine": false
  },
  "rules": {
    "accessor-pairs": 0,
    "array-callback-return": 1,
    "block-scoped-var": 1,
    "complexity": [
      0,
      11
    ],
    "consistent-return": 1,
    "curly": [
      1,
      "multi-line"
    ],
    "default-case": [
      1,
      {
        "commentPattern": "^no default$"
      }
    ],
    "dot-notation": [
      1,
      {
        "allowKeywords": true
      }
    ],
    "dot-location": 0,
    "eqeqeq": [
      1,
      "allow-null"
    ],
    "guard-for-in": 1,
    "id-blacklist": 0,
    "no-alert": 1,
    "no-caller": 1,
    "no-case-declarations": 1,
    "no-div-regex": 0,
    "no-else-return": 1,
    "no-empty-function": [
      1,
      {
        "allow": [
          "arrowFunctions",
          "functions",
          "methods"
        ]
      }
    ],
    "no-empty-pattern": 1,
    "no-extra-label": 1,
    "no-eq-null": 0,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-implicit-coercion": 0,
    "no-implied-eval": 1,
    "no-invalid-this": 0,
    "no-iterator": 1,
    "no-labels": [
      1,
      {
        "allowLoop": false,
        "allowSwitch": false
      }
    ],
    "no-lone-blocks": 1,
    "no-loop-func": 1,
    "no-magic-numbers": [
      0,
      {
        "ignore": [],
        "ignoreArrayIndexes": true,
        "enforceConst": true,
        "detectObjects": false
      }
    ],
    "no-multi-spaces": 1,
    "no-multi-str": 1,
    "no-native-reassign": 1,
    "no-new": 1,
    "no-new-func": 1,
    "no-new-wrappers": 1,
    "no-octal": 1,
    "no-octal-escape": 1,
    "no-param-reassign": [
      1,
      {
        "props": true
      }
    ],
    "no-process-env": 0,
    "no-proto": 1,
    "no-redeclare": 1,
    "no-restricted-syntax": [
      1,
      "DebuggerStatement",
      "ForInStatement",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-return-assign": 1,
    "no-script-url": 1,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-throw-literal": 1,
    "no-unmodified-loop-condition": 0,
    "no-unsafe-finally": 1,
    "no-unused-expressions": 1,
    "no-unused-labels": 1,
    "no-useless-call": 0,
    "no-useless-concat": 1,
    "no-useless-escape": 1,
    "no-void": 0,
    "no-warning-comments": [
      0,
      {
        "terms": [
          "todo",
          "fixme",
          "xxx"
        ],
        "location": "start"
      }
    ],
    "no-with": 1,
    "radix": 1,
    "vars-on-top": 1,
    "wrap-iife": [
      1,
      "outside"
    ],
    "yoda": 1,
    "no-cond-assign": [
      1,
      "always"
    ],
    "no-console": 1,
    "no-constant-condition": 1,
    "no-control-regex": 1,
    "no-debugger": 1,
    "no-dupe-args": 1,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-empty-character-class": 1,
    "no-empty": 1,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 0,
    "no-extra-parens": [
      0,
      "all",
      {
        "conditionalAssign": true,
        "nestedBinaryExpressions": false
      }
    ],
    "no-extra-semi": 1,
    "no-func-assign": 1,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 1,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    "no-unreachable": 1,
    "use-isnan": 1,
    "valid-jsdoc": 0,
    "valid-typeof": 1,
    "no-unexpected-multiline": 0,
    "comma-dangle": [
      1,
      "never"
    ],
    "max-depth": [
      0,
      4
    ],
    "max-params": [
      0,
      3
    ],
    "max-statements": [
      0,
      10
    ],
    "no-bitwise": 0,
    "no-plusplus": 0,
    "callback-return": 0,
    "global-require": 1,
    "handle-callback-err": 0,
    "no-mixed-requires": [
      0,
      false
    ],
    "no-new-require": 0,
    "no-path-concat": 0,
    "no-process-exit": 0,
    "no-restricted-modules": 0,
    "no-sync": 0,
    "import/no-unresolved": [
      1,
      {
        "commonjs": true
      }
    ],
    "array-bracket-spacing": [
      1,
      "never"
    ],
    "block-spacing": [
      1,
      "always"
    ],
    "brace-style": [
      1,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "camelcase": [
      1,
      {
        "properties": "never"
      }
    ],
    "comma-spacing": [
      1,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      1,
      "last"
    ],
    "computed-property-spacing": [
      1,
      "never"
    ],
    "consistent-this": 0,
    "eol-last": 1,
    "func-names": 1,
    "func-style": 0,
    "id-length": 0,
    "indent": [
      1,
      2,
      {
        "SwitchCase": 1,
        "VariableDeclarator": 1
      }
    ],
    "jsx-quotes": 0,
    "key-spacing": [
      1,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "keyword-spacing": [
      1,
      {
        "before": true,
        "after": true,
        "overrides": {
          "return": {
            "after": true
          },
          "throw": {
            "after": true
          },
          "case": {
            "after": true
          }
        }
      }
    ],
    "lines-around-comment": 0,
    "linebreak-style": 0,
    "max-len": [
      1,
      100,
      2,
      {
        "ignoreUrls": true,
        "ignoreComments": false
      }
    ],
    "max-nested-callbacks": 0,
    "max-statements-per-line": [
      0,
      {
        "max": 1
      }
    ],
    "new-cap": [
      1,
      {
        "newIsCap": true
      }
    ],
    "new-parens": 0,
    "newline-after-var": 0,
    "newline-before-return": 0,
    "newline-per-chained-call": [
      1,
      {
        "ignoreChainWithDepth": 3
      }
    ],
    "no-array-constructor": 1,
    "no-continue": 0,
    "no-inline-comments": 0,
    "no-lonely-if": 0,
    "no-mixed-spaces-and-tabs": 1,
    "no-multiple-empty-lines": [
      1,
      {
        "max": 1,
        "maxEOF": 1
      }
    ],
    "no-negated-condition": 0,
    "no-nested-ternary": 1,
    "no-new-object": 1,
    "no-spaced-func": 1,
    "no-ternary": 0,
    "no-trailing-spaces": 1,
    "no-underscore-dangle": [
      1,
      {
        "allowAfterThis": false
      }
    ],
    "no-unneeded-ternary": [
      1,
      {
        "defaultAssignment": false
      }
    ],
    "no-whitespace-before-property": 1,
    "object-curly-spacing": [
      1,
      "always"
    ],
    "one-var": [
      1,
      "never"
    ],
    "one-var-declaration-per-line": [
      1,
      "always"
    ],
    "operator-assignment": 0,
    "operator-linebreak": 0,
    "padded-blocks": [
      1,
      "never"
    ],
    "quote-props": [
      1,
      "as-needed",
      {
        "keywords": false,
        "unnecessary": true,
        "numbers": false
      }
    ],
    "quotes": [
      1,
      "single",
      "avoid-escape"
    ],
    "id-match": 0,
    "require-jsdoc": 0,
    "semi-spacing": [
      1,
      {
        "before": false,
        "after": true
      }
    ],
    "semi": [
      1,
      "always"
    ],
    "sort-vars": 0,
    "space-before-blocks": 1,
    "space-before-function-paren": [
      1,
      {
        "anonymous": "never",
        "named": "never"
      }
    ],
    "space-in-parens": [
      1,
      "never"
    ],
    "space-infix-ops": 1,
    "space-unary-ops": 0,
    "spaced-comment": [
      1,
      "always",
      {
        "exceptions": [
          "-",
          "+"
        ],
        "markers": [
          "=",
          "!"
        ]
      }
    ],
    "wrap-regex": 0,
    "init-declarations": 0,
    "no-catch-shadow": 0,
    "no-delete-var": 1,
    "no-implicit-globals": 0,
    "no-label-var": 0,
    "no-self-assign": 1,
    "no-shadow-restricted-names": 1,
    "no-shadow": 1,
    "no-undef-init": 0,
    "no-undef": 1,
    "no-undefined": 0,
    "no-unused-vars": [
      1,
      {
        "vars": "local",
        "args": "after-used"
      }
    ],
    "no-use-before-define": 1
  },
  "parserOptions": {
    "ecmaFeatures": {
      "globalReturn": true
    }
  },
  "plugins": [
    "import"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".json"
        ]
      }
    }
  },
  "ecmaFeatures": {}
}